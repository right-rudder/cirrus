---
import FAQs from "../FAQs.astro";
import SimpleSection from "./SimpleSection.astro";

const {
  title = "Frequently Asked Questions",
  subTitle = "Clear any doubts you might have",
  categories = undefined,
} = Astro.props;
---

<SimpleSection title={title} subTitle={subTitle} bg="bg-slate-100">
  <FAQs showCategory={true} categories={categories} />
</SimpleSection>
